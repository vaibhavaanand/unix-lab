1.date +"%D %T"


2.echo "enter a number "
read n
if test $n -gt 0
then
        echo "number is positive"
else

         echo "number is nagative" 
fi


3.echo " enter your marks"
read n
if [ "$n" -ge 90 ]
then
echo " you got A"
elif [ "$n" -ge 80 ]
then
        echo " you got B"
elif [ "$n" -ge 70 ]
then
        echo " you got C"
elif [ "$n" -ge 60]
then
        echo "you got D"
else
        echo "you are failed"
fi


